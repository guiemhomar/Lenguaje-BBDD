<%- include("header") %>

<div class="container">
  <h2>Lista de Usuarios</h2>
  <ul>
    <% users.forEach(user => { %>
      <li>
        <a href="/usuari/<%= user.id %>"><%= user.nom %></a>
      </li>
    <% }); %>
  </ul>
  <a href="/usuari" class="btn btn-primary">Crear Usuario</a>
</div>

<%- include("footer") %>
